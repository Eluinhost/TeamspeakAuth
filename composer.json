{
    "name": "PublicUHC/TeamspeakAuth",
    "description": "'Authenticate' Teamspeak 3 accounts",
    "minimum-stability": "stable",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Eluinhost",
            "email": "graham_howden1@yahoo.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eluinhost/skin-cache"
        }
    ],
    "require": {
        "planetteamspeak/ts3-php-framework": "1.1.23",
        "symfony/http-foundation": "2.5",
        "symfony/routing": "2.5",
        "symfony/config": "2.5",
        "symfony/yaml": "2.5",
        "symfony/http-kernel": "2.5",
        "symfony/dependency-injection": "2.5",
        "symfony/templating": "2.5",
        "symfony/twig-bridge": "2.5",
        "doctrine/orm": "2.4.*",
        "eluinhost/skin-cache": "dev-master as 0.1"
    },
    "autoload": {
        "psr-4": {"PublicUHC\\TeamspeakAuth\\": "src/PublicUHC/TeamspeakAuth"}
    },
    "scripts": {
        "post-install-cmd": [
            "PublicUHC\\TeamspeakAuth\\Composer\\CopyNewConfig::postInstallCommand"
        ],
        "post-update-cmd": [
            "PublicUHC\\TeamspeakAuth\\Composer\\CopyNewConfig::postUpdateCommand"
        ]
    },
    "extra": {
        "ymlinstall": {
            "config": "config/config.yml",
            "dist": "src/config.yml.dist"
        }
    }
}
